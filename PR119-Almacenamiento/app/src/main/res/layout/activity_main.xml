<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/interno"
        android:id="@+id/btnInterno"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/externo_privado"
        android:id="@+id/btnExternoPrivado"
        android:layout_below="@+id/btnInterno"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/marginTop" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/externo_publico"
        android:id="@+id/btnExternoPublico"
        android:layout_below="@+id/btnExternoPrivado"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/marginTop" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cache_interno"
        android:id="@+id/btnCacheInterno"
        android:layout_below="@+id/btnExternoPublico"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/marginTop" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cache_externo"
        android:id="@+id/btnCacheExterno"
        android:layout_below="@+id/btnCacheInterno"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/marginTop" />
</RelativeLayout>
